#include<stdio.h>
#include<math.h>
void main(){
int a, b, c;
int d,r1,r2;
printf("Enter the Coeficients: ");
scanf("%d %d %d",&a, &b, &c\n);
d=b*b-4*a*c;
if(d>0){
r1=(-b+sqrt(d))/2;
r2=(-b-sqrt(d))/2;
printf("Roots are: %d , %d\n",r1 , r2);
}
else if(d==0){
r1=r2=-b/(2*a);
printf("Roots are: %d , %d\n",r1 , r2);
}
else{
printf("roots are complex.\n");
}
}
